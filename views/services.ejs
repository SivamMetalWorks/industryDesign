<script>
  const navItems = [
    "about",
    "home",
    "contact",
    "products",
    "services",
    "gallery",
  ];
  if (window.location.pathname == "/services") {
    navItems.map((item) => {
      if (item == "services") {
        $(`.${item}`)
          .removeClass("text-white")
          .css("color", "rgb(253, 209, 49)")
          .addClass("font-2");
      } else {
        $(`.${item}`).removeClass("font-2").removeAttr("css");
      }
    });
  }
</script>

